  "compilerOptions": {
    "types": ["bun-types"],
    "target": "esnext",
    "module": "commonjs",
    "lib": ["esnext", "es2019", "es2017", "es7", "es6", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
